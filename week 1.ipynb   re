    "5. Create a bar chart of number of alarms per day."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "id": "4d45c19c-21e2-4117-8d7c-acc3981219b8",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 800x500 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "\n",
    "#df = pd.read_excel(\"telecom_alarm_data.xlsx\", sheet_name=\"Data\")\n",
    "\n",
    "\n",
    "df[\"Alarm Occurred Time\"] = pd.to_datetime(df[\"Alarm Occurred Time\"])\n",
    "\n",
    "\n",
    "df[\"Alarm Date\"] = df[\"Alarm Occurred Time\"].dt.date\n",
    "\n",
    "\n",
    "alarms_per_day = df[\"Alarm Date\"].value_counts().sort_index().reset_index()\n",
    "alarms_per_day.columns = [\"Date\", \"Number of Alarms\"]\n",
    "\n",
    "\n",
    "plt.figure(figsize=(8, 5))\n",
    "plt.bar(alarms_per_day[\"Date\"].astype(str), alarms_per_day[\"Number of Alarms\"], color=\"slateblue\")\n",
    "\n",
    "plt.title(\"Number of Alarms per Day\")\n",
    "plt.xlabel(\"Date\")\n",
    "plt.ylabel(\"Number of Alarms\")\n",
    "plt.xticks(rotation=45)\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },

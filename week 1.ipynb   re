    "\n",
    "import pandas as pd \n",
    "\n",
    "df = pd.read_excel(r\"C:\\Users\\Cavidan\\Desktop\\telecom_alarm_data.xlsx\",sheet_name=2)\n",
    "\n",
    "df['Alarm Occurred Time'] = pd.to_datetime(df['Alarm Occurred Time'])\n",
    "df['Alarm Resolved Time'] = pd.to_datetime(df['Alarm Resolved Time'])\n",
    "\n",
    "df['Alrm dur'] = (df['Alarm Resolved Time'] - df['Alarm Occurred Time']).dt.total_seconds() / 60\n",
    "\n",
    "print(df[['Site ID','Site Name','Alarm Occurred Time', 'Alarm Resolved Time', 'Alrm dur']].head(50))\n"

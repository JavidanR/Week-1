   "source": [
    "6. Group alarms by region and count how many alarms happened during night (00:00–06:00)."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 34,
   "id": "7255ed9a-65c6-4203-8b62-2f4626fce9f7",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "        Region  Night Alarm Count\n",
      "0         Baku                  3\n",
      "1        Ganja                  1\n",
      "2     Lankaran                  2\n",
      "3  Mingachevir                  5\n",
      "4        Sheki                  1\n",
      "5     Sumqayit                  1\n"
     ]
    }
   ],
   "source": [
    "import pandas as pd\n",
    "from datetime import time\n",
    "\n",
    "\n",
    "df = pd.read_excel(\"telecom_alarm_data.xlsx\", sheet_name=\"Data\")\n",
    "\n",
    "\n",
    "\n",
    "night_alarms = df[df[\"Alarm Occurred Time\"].dt.time.between(time(0, 0), time(6, 0))]\n",
    "\n",
    "\n",
    "night_alarms_per_region = night_alarms.groupby(\"Region\")[\"Alarm Occurred Time\"].count().reset_index()\n",
    "\n",
    "\n",
    "night_alarms_per_region.columns = [\"Region\", \"Night Alarm Count\"]\n",
    "\n",
    "\n",
    "print(night_alarms_per_region)"
   ]
  },
